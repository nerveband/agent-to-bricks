          "_cssCustom": ".fr-timeline-bravo {\n\t--channel-height: 80px;\n  --channel-line-color: var(--neutral-light);\n  --channel-indicator-size: 20px;\n  --channel-indicator-color: var(--primary);\n}\n\n.fr-timeline-bravo__slider::before {\n\tcontent: '';\n  width: 100vw;\n  height: var(--channel-line-height, 2px);\n  background-color: var(--channel-line-color, var(--neutral-light));\n  position: absolute;\n  top: 50%;\n  left: 50%;\n  translate: -50% -50%;\n}\n\n.fr-timeline-bravo__content-wrapper::after {\n  content: '';\n  width: var(--channel-indicator-size, 20px);\n  height: var(--channel-indicator-size, 20px);\n  background-color: var(--channel-indicator-color, var(--primary));\n  border-radius: 50vw;\n  position: absolute;\n  inset-block-start: 50%;\n  translate: 0 -50%;\n  z-index: 1;\n}\n\n.fr-timeline-bravo__timeline-card:nth-child(even) .fr-timeline-bravo__media-wrapper,\n.fr-timeline-bravo__timeline-card:nth-child(even) .fr-timeline-bravo__date {\n\torder: -1;\n}",
