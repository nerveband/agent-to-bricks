        "name": "fr-vertical-header-bravo",
        "settings": {
          "_cssCustom": ".fr-vertical-header-bravo, #brx-header, #brx-content, #brx-footer {\n  --header-width: 6em;\n  --header-color: var(--bg-light);\n  --burger-color: var(--text-dark);\n  --icon-size: 1.5em;\n  --icon-color: var(--primary);\n  --icon-color-hover: var(--primary-hover);\n}\n\n.fr-vertical-header-bravo {\n  background-color: var(--header-color);  \n}\n\n.fr-vertical-header-bravo .fr-vertical-header-bravo__body {\n  display: none;\n}\n\n/* Social Icon Styling */\n.fr-vertical-header-bravo .fr-vertical-header-bravo__footer svg {\n\tfill: var(--icon-color);\n  height: var(--icon-size);\n  width: var(--icon-size);\n}\n.fr-vertical-header-bravo .fr-vertical-header-bravo__footer svg:hover {\n\tfill: var(--icon-color-hover);\n}\n\n/* Set width when Mobile Toggle display */\n@media (min-width: 768px) {\n\n  .fr-vertical-header-bravo {\n    flex-direction: column!important;\n    justify-content: space-between;\n    align-items: center;\n    row-gap: var(--content-gap);  \n    padding: var(--space-xl) var(--space-xs)!important;\n  }\n\n  .fr-vertical-header-bravo .fr-vertical-header-bravo__head {\n    align-items: center;\n  }\n  \n  .fr-vertical-header-bravo .fr-vertical-header-bravo__body {\n    display: flex!important;\n  }  \n\n  .fr-vertical-header-bravo .fr-vertical-header-bravo__footer {\n    flex-direction: column!important;\n    margin-block-start: auto; \n  }\n\n  #brx-header {\n    display: flex;\n    position: fixed;\n    isolation: isolate;  \n    top: 0;\n    bottom: 0;  \n    left: 0;  \n    width: var(--header-width);\n    z-index: 1001;\n  }\n\n  #brx-content, #brx-footer {\n    margin-left: var(--header-width);\n    width: auto;\n  }\n\n}",
