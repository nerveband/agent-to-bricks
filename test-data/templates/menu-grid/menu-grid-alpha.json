        "name": "fr-menu-grid-alpha",
        "settings": {
          "_cssCustom": ".fr-menu-grid-alpha {\n  --row-gap: var(--grid-gap);\n  --col-gap: var(--space-xl);\n  --featured-box-offset: .75em;\n}\n\n/* Featured Item Styling */\n\n.fr-menu-grid-alpha {\n\tposition: relative;\n  isolation: isolate;\n  gap: calc(var(--row-gap) + var(--featured-box-offset)) var(--col-gap);\n}",
          "_cssCustom:mobile_landscape": ".fr-menu-grid-alpha {\n  grid-template-columns: 1fr;\n}",
          "_gridTemplateColumns": "var(--grid-2)",
          "_gridTemplateRows": "1fr",
          "_display": "grid",
          "_gridTemplateColumns:tablet_portrait": "var(--grid-1)"
        },
