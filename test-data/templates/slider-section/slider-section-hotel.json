          "executeCode": true,
          "javascriptCode": "document.addEventListener('DOMContentLoaded', function () {\n  if (typeof Splide === 'undefined') return;\n\n  var splideElements = document.querySelectorAll('.splide');\n  if (!splideElements.length) return;\n\n  var splideInstances = [];\n  var syncGroups = {}; // syncId -> [Splide instances]\n\n  splideElements.forEach(function (splideEl) {\n    // Skip if already initialized (e.g. duplicate script in duplicated markup)\n    if (splideEl.getAttribute('data-fr-slider-initialized') === 'true') return;\n\n    var splide = new Splide(splideEl, {\n      arrows: false,\n    });\n\n    // Container: the block that holds the slider and controls (e.g. .fr-slider-hotel)\n    var container = splideEl.closest('.fr-slider-hotel') || splideEl.parentElement;\n    // Each slider has its own data-fr-slider-sync-id \u2013 used to find matching nav elements\n    var syncId = splideEl.getAttribute('data-fr-slider-sync-id') || '';\n\n    var SlideNumber = function (Splide, Components, options) {\n      options = options || { doubleDigit: false };\n      var scope = options.scope || document;\n      var navSyncId = options.navSyncId || '';\n      var track = Components.Elements.track;\n      var currentSlideEl;\n      var totalSlidesEl;\n      var navContainer;\n\n      function mount() {\n        // Progress container: match to slider via data-fr-slider-nav-sync-id (same ID as slider)\n        if (navSyncId) {\n          navContainer = scope.querySelector('[data-fr-slider-nav-sync-id=\"' + navSyncId + '\"][data-fr-slider-nav-type=\"progress\"]');\n        }\n        if (!navContainer) {\n          navContainer = scope.querySelector('.fr-slider-hotel__progress, [data-fr-slider-nav-type=\"progress\"]');\n        }\n        if (!navContainer) {\n          navContainer = document.createElement('div');\n          navContainer.className = 'fr-slider-hotel__progress';\n          if (track && track.parentElement) {\n            track.parentElement.insertBefore(navContainer, track.nextSibling);\n          }\n        }\n\n        currentSlideEl = navContainer.querySelector('.current-slide');\n        totalSlidesEl = navContainer.querySelector('.total-slides');\n\n        if (!currentSlideEl) {\n          currentSlideEl = document.createElement('div');\n          currentSlideEl.className = 'current-slide';\n          navContainer.appendChild(currentSlideEl);\n        }\n        if (!totalSlidesEl) {\n          totalSlidesEl = document.createElement('div');\n          totalSlidesEl.className = 'total-slides';\n          navContainer.appendChild(totalSlidesEl);\n        }\n\n        totalSlidesEl.textContent = formatNumber(Splide.length);\n        Splide.on('mounted move moved updated', update);\n\n        // Arrows: find the correct block via data-fr-slider-nav-sync-id (nextPrev)\n        var arrowsContainer = null;\n        if (navSyncId) {\n          arrowsContainer = scope.querySelector('[data-fr-slider-nav-sync-id=\"' + navSyncId + '\"][data-fr-slider-nav-type=\"nextPrev\"]');\n        }\n        if (!arrowsContainer) {\n          arrowsContainer = scope;\n        }\n        var prevArrow = arrowsContainer.querySelector('.fr-slider__custom-arrow--prev');\n        var nextArrow = arrowsContainer.querySelector('.fr-slider__custom-arrow--next');\n        if (prevArrow) {\n          prevArrow.addEventListener('click', function () { Splide.go('<'); });\n        }\n        if (nextArrow) {\n          nextArrow.addEventListener('click', function () { Splide.go('>'); });\n        }\n\n        // Progress buttons: in the already found navContainer\n        var progressButtonsContainer = navContainer.querySelector('.fr-slider__progress-buttons');\n        if (progressButtonsContainer) {\n          progressButtonsContainer.addEventListener('click', function (event) {\n            var button = event.target.closest('.fr-slider__progress-button');\n            if (button) {\n              var index = parseInt(button.getAttribute('data-index'), 10);\n              Splide.go(index);\n            }\n          });\n        }\n\n        update();\n      }\n\n      function update() {\n        if (!currentSlideEl) return;\n        var currentIndex = Splide.index + 1;\n        currentSlideEl.textContent = formatNumber(currentIndex);\n      }\n\n      function formatNumber(number) {\n        return options.doubleDigit ? String(number).padStart(2, '0') : String(number);\n      }\n\n      return { mount: mount };\n    };\n\n    splide.on('mounted', function () {\n      var slideNumberInstance = SlideNumber(splide, splide.Components, {\n        doubleDigit: true,\n        scope: container,\n        navSyncId: syncId,\n      });\n      slideNumberInstance.mount();\n    });\n\n    splide.mount();\n    splideEl.setAttribute('data-fr-slider-initialized', 'true');\n    splideInstances.push({ splide: splide, el: splideEl, container: container });\n\n    // Collect sync group (same syncId = sliders run in sync)\n    if (syncId) {\n      if (!syncGroups[syncId]) syncGroups[syncId] = [];\n      syncGroups[syncId].push(splide);\n    }\n  });\n\n  // Sync: on \"moved\", set all other sliders in the same group to the same index\n  Object.keys(syncGroups).forEach(function (syncId) {\n    var group = syncGroups[syncId];\n    if (group.length < 2) return;\n\n    var syncing = false; // prevents re-entry when calling go() programmatically\n    group.forEach(function (splide) {\n      splide.on('moved', function (newIndex) {\n        if (syncing) return;\n        syncing = true;\n        group.forEach(function (other) {\n          if (other !== splide && other.index !== newIndex) {\n            other.go(newIndex);\n          }\n        });\n        syncing = false;\n      });\n    });\n  });\n});\n"
