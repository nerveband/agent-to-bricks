          "_cssCustom": ".hero-cali__bg {\n  translate: 0% -200px;\n  height: 90vh;\n}\n\n/* Begin Fade Bottom */\n.hero-cali__bg {\n  --fade-amount: 85%;\n  -webkit-mask-image:\n    linear-gradient(to top, transparent 20%, black var(--fade-amount));\n  mask-image:\n    linear-gradient(to top, transparent 20%, black var(--fade-amount));\n\n}\n/* End Fade Bottom */\n\n/* Auto-responsive columns */\n.hero-cali__bg {\n  --col-count: 6;\n  --col-min-width: 0;\n  --col-rule-style: solid;\n  --col-rule-width: 0;\n  --col-rule-color: var(--black);\n  --col-gap: var(--grid-gap);\n  --row-gap: var(--grid-gap);\n\n  @media (max-width: 992px) {\n    --col-count: 4;\n  }\n  @media (max-width: 767px) {\n    --col-count: 3;\n  }\n}\n\n.hero-cali__bg {\n  display: block !important;\n  columns: var(--col-count) var(--col-min-width);\n  column-rule-style: var(--col-rule-style);\n  column-rule-width: var(--col-rule-width);\n  column-rule-color: var(--col-rule-color);\n  gap: var(--col-gap);\n}\n\n.hero-cali__bg > * {\n  break-inside: avoid;\n  margin-block-end: var(--row-gap);\n}"
