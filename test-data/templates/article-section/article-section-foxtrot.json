      {
        "id": "888a4e",
        "name": "code",
        "parent": "543ae3",
        "children": [],
        "settings": {
          "code": "<script>\n  // Access settings in Bricks > Settings > Builder Access \n  // Allow Code Execution for Administrator role\n  // Execute the code below in this field\n  // Note: Only one Accessibility Code is needed for each card type.\n  // If you have imported more than one Article Section Foxtrot, remove any extra accessibility code.\n\n  // First, select all the card elements on the page\n  const deltaCards = document.querySelectorAll('.fr-article-card-foxtrot');\n\n  // Check if hover is supported on the user's device\n  const isHoverSupported = window.matchMedia(\"(hover: hover)\").matches;\n\n  // Add event listeners or directly set styles based on device capabilities\n  deltaCards.forEach(card => {\n    // Find specific elements inside each card\n    const link = card.querySelector('.fr-article-card-foxtrot__title a');\n    const bodyCard = card.querySelector('.fr-article-card-foxtrot__body');\n\n    // If hover is supported, add focus and blur event listeners\n    if (isHoverSupported) {\n      link.addEventListener('focus', () => {\n        // When the link is focused, make the body visible\n        bodyCard.classList.add('fr-article-card-foxtrot--visible');\n      });\n\n      link.addEventListener('blur', () => {\n        // When focus is lost, hide the body again\n        bodyCard.classList.remove('fr-article-card-foxtrot--visible');\n      });\n    } else {\n      // If hover is not supported, always keep the body visible\n      bodyCard.classList.add('fr-article-card-foxtrot--visible');\n    }\n  });\n</script>",
