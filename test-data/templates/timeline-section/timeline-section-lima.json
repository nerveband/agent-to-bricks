      {
        "id": "ngkrdy",
        "name": "fr-timeline-lima",
        "settings": {
          "_rowGap": "var(--container-gap)",
          "_position": "relative",
          "_cssCustom": ".fr-timeline-lima {\n\t--timeline-gap: var(--container-gap);\n  --timeline-content-gap: var(--fr-card-gap);\n  --center-line-color: var(--neutral-light);\n  --center-line-width: 0.125em;\n  --icon-size: 1.5em;\n}\n\n.fr-timeline-lima .fr-timeline-lima__list-item {\n  display: grid;\n  grid-template-columns: repeat(12, 1fr);\n\tgrid-template-rows: var(--grid-1);\n  align-items: stretch;\n}\n\n.fr-timeline-lima .fr-timeline-lima__date-wrapper {\n\tgrid-column: 1 / 4;\n}\n\n.fr-timeline-lima .fr-timeline-lima__icon-wrapper {\n\tgrid-column: 4 / 5;\n}\n\n.fr-timeline-lima .fr-timeline-lima__content {\n\tgrid-column: 5 / end;\n}\n\n/* Line Styling */\n.fr-timeline-lima .fr-timeline-lima__icon-wrapper::before {\n  content: \"\";\n  width: var(--center-line-width);  \n  background-color: var(--center-line-color);\n  background-size: cover;\n  position: absolute;\n  top: 0;\n  bottom: calc(var(--timeline-gap) * -1);\n}\n\n.fr-timeline-lima li:last-child .fr-timeline-lima__icon-wrapper::before {\n\tbackground: transparent;\n}\n\n/* Visual Fix inside the Builder */\n.fr-timeline-lima .brx-draggable.is-empty {\n  min-width: unset;\n  min-height: unset;\n}",
          "_cssCustom:mobile_landscape": ".fr-timeline-lima .fr-timeline-lima__date-wrapper {\n\tgrid-column: 2 / end;\n\tgrid-row: 1;\n}\n\n.fr-timeline-lima .fr-timeline-lima__content {\n\tgrid-column: 2 / end;\n\tgrid-row: 2;\n}\n\n.fr-timeline-lima .fr-timeline-lima__icon-wrapper {\n  grid-area: 1 / 1 / end;\n}"
        },
