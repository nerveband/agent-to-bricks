          "_position": "relative",
          "_cssCustom": ".fr-content-section-november {\n  --grid: var(--grid-12); /* No touchey */\n  --min-height: calc(100vh - 200px);\n  --content-box-width: 720px; /* Optionally limit width in px) */  \n  --overlap: 2; /* Degree of overlap (< media ratio) */\n  --media-ratio: 7; /* Out of 12 (\"7\" is 50%) */  \n  --media-position: 50% 50%; /* Object-position control */\n\t--media-width: calc((50vw + (var(--media-ratio) - 6) * (var(--content-width) / 12))); \n  contain: paint;\n}\n\n.fr-content-section-november__media-wrapper {\n\tgrid-column: 1 / var(--media-ratio);\n  grid-row: 1;\n  align-items: flex-end;\n}\n\n.fr-content-section-november__content-wrapper {\n  grid-column: calc(var(--media-ratio) - var(--overlap)) / -1;\n\tgrid-row: 1;\n}\n\n[data-layout=\"alt\"] .fr-content-section-november__content-wrapper {\n  grid-column: 1 / calc(var(--media-ratio) + var(--overlap) - 1);\n\tgrid-row: 1;\n}\n[data-layout=\"alt\"] .fr-content-section-november__media-wrapper {\n\tgrid-column: calc(13 - var(--media-ratio)) / end;\n  grid-row: 1;\n  align-items: flex-start;  \n}",
