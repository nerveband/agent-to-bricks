          "_cssCustom": ".image-group-north {\n  --gap: var(--space-xs);\n}\n\n/* Rows */\n.image-group-north__row[data-row=\"1\"] {\n  grid-template-columns: .75fr 1.5fr 1fr .5fr;\n  align-items: end;\n}\n.image-group-north__row[data-row=\"2\"] {\n  grid-template-columns: .5fr 1fr 1.5fr .75fr;\n}\n\n/* Aspect Ratios */\n\n[data-row=\"1\"] {\n  \n  .image-group-north__image:first-child,\n  .image-group-north__image:last-child {\n    --aspect-ratio: 1/1;\n  }\n\n  .image-group-north__image:nth-child(2) {\n    --aspect-ratio: 16/9;\n  }\n  .image-group-north__image:nth-child(3) {\n    --aspect-ratio: 6/4;\n  }\n  \n}\n\n[data-row=\"2\"] {\n  \n  .image-group-north__image:first-child,\n  .image-group-north__image:last-child {\n    --aspect-ratio: 1/1;\n  }\n\n  .image-group-north__image:nth-child(2) {\n    --aspect-ratio: 6/4;\n  }\n  .image-group-north__image:nth-child(3) {\n    --aspect-ratio: 16/9;\n  }\n  \n}\n\n/* Overflow */\n\n.image-group-north__row {\n  --offset: 2em;\n  max-width: calc(100% + var(--offset));\n  width: calc(100% + var(--offset));\n  translate: calc(var(--offset) / 2 * -1) 0;\n}",
