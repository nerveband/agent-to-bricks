        "name": "fr-profile-card-golf",
        "settings": {
          "_position": "relative",
          "_cssCustom": ".fr-profile-card-golf {\n\t--overlay-color: var(--black-trans-60);\n  --transition-duration: .3s;\n  --transition-function: ease-in-out;\n  --transition-delay: .05s;\n  --social-icon-size: 1.15em;\n  --social-icon-color: var(--white);\n  --social-icon-gap: .25em;\n}\n\n.fr-profile-card-golf {\n\toverflow: hidden;\n}\n\n.fr-profile-card-golf:focus-within :focus {\n\toutline: none;\n  box-shadow: none;\n}\n\n.fr-profile-card-golf:focus-within {\n\tbox-shadow: 0 0 40px 0px var(--black-trans-10);\n  outline: none !important;\n}\n\n@media (min-width:767px) {\n\n  /* Media */\n\n  .fr-profile-card-golf__media {\n    transition: var(--transition-duration) var(--transition-function);\n  }\n\n  .fr-profile-card-golf.is-hover .fr-profile-card-golf__media,\n  .fr-profile-card-golf:focus-within .fr-profile-card-golf__media{\n    scale: 1.1;\n  }\n\n  /* Overlay */\n\n  .fr-profile-card-golf__overlay {\n    opacity: 0;\n    transition: calc(var(--transition-duration) + .35s) var(--transition-function);\n  }\n\n  .fr-profile-card-golf.is-hover .fr-profile-card-golf__overlay,\n  .fr-profile-card-golf:focus-within .fr-profile-card-golf__overlay{\n    opacity: 1;\n  }\n\n  /*** Content ***/\n\n  /* Name */\n\n  .fr-profile-card-golf__name {\n    transition: calc(var(--transition-duration)) var(--transition-function);\n    opacity: 0;\n  }\n\n  .fr-profile-card-golf.is-hover .fr-profile-card-golf__name,\n  .fr-profile-card-golf:focus-within .fr-profile-card-golf__name{\n    opacity: 1;\n  }\n\n  /* Title */\n\n  .fr-profile-card-golf__title {\n    translate: 0% 100%;\n    opacity: 0;\n    transition: calc(var(--transition-duration) - .05s) var(--transition-function);\n  }\n\n  .fr-profile-card-golf.is-hover .fr-profile-card-golf__title,\n  .fr-profile-card-golf:focus-within .fr-profile-card-golf__title {\n    translate: 0% 0%;\n    opacity: 1;\n  }\n\n  /* Social Icons */\n\n  .fr-profile-card-golf__social-icon {\n    transition: var(--transition-duration) var(--transition-function);\n    opacity: 0;\n    translate: 0% 100%;\n  }\n\n  .fr-profile-card-golf.is-hover .fr-profile-card-golf__social-icon,\n  .fr-profile-card-golf:focus-within .fr-profile-card-golf__social-icon {\n    translate: 0% 0%;\n    opacity: 1;\n  }\n\n  .fr-profile-card-golf__social-icon:nth-child(1) {\n    transition-delay: var(--transition-delay);\n  }\n  .fr-profile-card-golf__social-icon:nth-child(2) {\n    transition-delay: calc(var(--transition-delay) + .05s);\n  }\n  .fr-profile-card-golf__social-icon:nth-child(3) {\n    transition-delay: calc(var(--transition-delay) + .075s);\n  }\n\n}",
