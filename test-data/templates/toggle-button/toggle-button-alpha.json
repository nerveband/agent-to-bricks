          "_cssCustom": ".fr-toggle-button-alpha {\n  /* Button Style */\n  --button-background-color: var(--primary);\n  --button-background-color-hover: var(--primary-hover);\n  --gap: calc(var(--content-gap) / 3);\n  /* Icon Style */\n  --icon-size: var(--text-l);\n  --icon-color: var(--white-trans-70);\n  --icon-color-hover: var(--white);\n  /* Label Style */\n  --label-color: var(--white-trans-70);\n  --label-color-hover: var(--white);\n  /* Animation */\n  --transition-duration: .25s;\n}\n\n/* Button Styling */\n\n.fr-toggle-button-alpha {\n  background: var(--button-background-color);\n\tposition: relative;\n  display: grid;\n  grid-template-columns: auto minmax(0, 1fr);\n  gap: var(--gap);\n  align-items: center;\n  border-color: var(--button-background-color);\n}\n\n.fr-toggle-button-alpha:hover {\n\tbackground: var(--button-background-color-hover);\n  border-color: var(--button-background-color-hover);\n}\n\n/* Icon & Label Styling */\n\n.fr-toggle-button-alpha .fr-toggle-button-alpha__icon {\n\tfill: var(--icon-color);\n}\n.fr-toggle-button-alpha:hover .fr-toggle-button-alpha__icon {\n\tfill: var(--icon-color-hover);\n}\n\n.fr-toggle-button-alpha .fr-toggle-button-alpha__label {\n\tcolor: var(--label-color);\n}\n.fr-toggle-button-alpha:hover .fr-toggle-button-alpha__label {\n\tcolor: var(--label-color-hover);\n}\n\n/* Animate */\n\n.fr-toggle-button-alpha .fr-toggle-button-alpha__icon--close,\n.fr-toggle-button-alpha .fr-toggle-button-alpha__label--close,\n.fr-toggle-button-alpha .fr-toggle-button-alpha__icon--open,\n.fr-toggle-button-alpha .fr-toggle-button-alpha__label--open {\n\ttransition: translate var(--transition-duration) ease;\n}\n\n/* Is Closed */\n\n.fr-toggle-button-alpha .fr-toggle-button-alpha__icon--close {\n\tposition: absolute;\n  translate: 0 -200%;\n  opacity: 0;\n}\n\n.fr-toggle-button-alpha .fr-toggle-button-alpha__label--close {\n\tposition: absolute;\n  translate: 0 200%;\n  opacity: 0;\n}\n\n/* Is Open */\n\n.fr-toggle-button-alpha.is-open .fr-toggle-button-alpha__icon--open {\n\tposition: absolute;\n  translate: 0 200%;\n  opacity: 0;\n}\n\n.fr-toggle-button-alpha.is-open .fr-toggle-button-alpha__label--open {\n\tposition: absolute;\n  translate: 0 -200%;\n  opacity: 0;\n}\n\n.fr-toggle-button-alpha.is-open .fr-toggle-button-alpha__icon--close,\n.fr-toggle-button-alpha.is-open .fr-toggle-button-alpha__label--close {\n\tposition: relative;\n  translate: 0% 0%;\n  opacity: 1;\n}",
